<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style gss="true">
	.content-row {
		margin-top : 90px;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Container fluid="true">
			<b:Row addStyleNames="{style.content-row}">
				<b:Column size="LG_6">
					<c:CellTable ui:field="cellTable"/>
				</b:Column>
				<b:Column size="LG_6">
					<g:SimplePanel ui:field="panel"></g:SimplePanel>
				</b:Column>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 