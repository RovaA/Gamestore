<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"  xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style gss="true">
	.content-row {
		margin-top : 50px;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Container fluid="true">
			<b:Row addStyleNames="{style.content-row}">
				<b:Column size="LG_12">
					<g:VerticalPanel ui:field="panel"></g:VerticalPanel>
				</b:Column>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 