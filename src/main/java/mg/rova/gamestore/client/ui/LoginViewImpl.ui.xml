<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:e="urn:import:mg.rova.gamestore.client.editor" xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<b:Container>
			<b:Row>
				<b:Row>
					<b:PageHeader text="Login" />
				</b:Row>
				<b:Row>
					<b:Column size="LG_2">
						<b:Label text="Username : "></b:Label>
					</b:Column>
					<b:Column size="LG_10">
						<b:TextBox ui:field="usernameTextBox" placeholder="Username..."></b:TextBox>
					</b:Column>
				</b:Row>
				<b:Row>
					<b:Column size="LG_2">
						<b:Label text="Password : "></b:Label>
					</b:Column>
					<b:Column size="LG_10">
						<b:TextBox ui:field="passwordTextBox" placeholder="Password"></b:TextBox>
					</b:Column>
				</b:Row>
				<b:Row>
					<m:MaterialButton ui:field="loginButton" text="Send" backgroundColor="blue"></m:MaterialButton>
					<m:MaterialButton ui:field="cancelButton" text="Cancel" backgroundColor="red"></m:MaterialButton>
				</b:Row>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 